#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/rollup@4.9.1/node_modules/rollup/dist/bin/node_modules:/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/rollup@4.9.1/node_modules/rollup/dist/node_modules:/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/rollup@4.9.1/node_modules/rollup/node_modules:/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/rollup@4.9.1/node_modules:/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/rollup@4.9.1/node_modules/rollup/dist/bin/node_modules:/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/rollup@4.9.1/node_modules/rollup/dist/node_modules:/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/rollup@4.9.1/node_modules/rollup/node_modules:/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/rollup@4.9.1/node_modules:/mnt/d/coderwhy系统课/代码/javascript/数据可视化/05_数据可视化平台/PowerView/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../dist/bin/rollup" "$@"
fi
